    "def BC(n, k):\n",
    "    if k == 0 or k == n:\n",
    "        return 1\n",
    "    return BC(n-1, k-1) + BC(n-1, k)\n",
    "\n",
    "n, k = list(map(int, input().split()))\n",
    "print(BC(n, k))"
   ]
  }
