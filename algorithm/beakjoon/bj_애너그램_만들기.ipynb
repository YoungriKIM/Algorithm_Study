   "source": [
    "n = list(input())\n",
    "m = list(input())\n",
    "# n = 'qeetyu'\n",
    "# m = 'deeeetyu'\n",
    "\n",
    "cnt = 0\n",
    "length = len(n) + len(m)\n",
    "\n",
    "for cha in n :\n",
    "    if cha in m:\n",
    "        cnt += 1\n",
    "        idx = m.index(cha)\n",
    "        del m[idx]\n",
    "\n",
    "if cnt:\n",
    "    print(length - (cnt * 2))\n",
    "else : print(0)\n"
   ]
  }
