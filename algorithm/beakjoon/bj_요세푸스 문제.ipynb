   "source": [
    "p, k = map(int, input().split())\n",
    "p_l = list(range(1, p+1))\n",
    "\n",
    "a, n= [], k-1\n",
    "\n",
    "while len(p_l):\n",
    "    if n < len(p_l):\n",
    "        a.append(str(p_l.pop(n)))\n",
    "        n += (k-1)\n",
    "    else:\n",
    "        n = n%len(p_l)\n",
    "\n",
    "print(f\"<{', '.join(a)}>\")"
   ]
  }
