   "source": [
    "# 최대공약수, 최소 공배수 구하기\n",
    "\n",
    "def 최대공약수(a, b):\n",
    "    if a < b:\n",
    "        a, b = b, a\n",
    "    while b != 0:\n",
    "        a, b = b, a % b\n",
    "    return a    \n",
    "\n",
    "def 최소공배수(a, b):\n",
    "    return a * b // 최대공약수(a, b)\n",
    "\n",
    "a, b = map(int, input().split())\n",
    "print(최대공약수(a, b))\n",
    "print(최소공배수(a, b))"
   ]
  }
 ],
